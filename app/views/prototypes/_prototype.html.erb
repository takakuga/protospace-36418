<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype.id) %>
  <div class="card__body">
    <%= link_to "プロトタイプのタイトル", prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= "プロトタイプのキャッチコピー" %>
    </p>
    <%= link_to "by プロトタイプの投稿者名", prototype_path(prototype.id), class: :card__user %>
  </div>
  <%= image_tag prototype.image, class: 'prototype-image' if prototype.image.attached? %>
</div>